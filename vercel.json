{
  "version": 2,
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/next",
      "config": {
        "includeFiles": ["../data/names_to_wallets.json"]
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
