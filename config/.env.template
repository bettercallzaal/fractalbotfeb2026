# Discord Bot Configuration
# Replace with your own Discord bot token
DISCORD_TOKEN=your_discord_token_here

# Application ID and Client Secret (optional)
# APPLICATION_ID=your_application_id
# CLIENT_SECRET=your_client_secret

# Alchemy API Key (optional)
# ALCHEMY_API_KEY=your_alchemy_api_key

# ZAO Role ID (Required for ZAO features)
# Right-click the ZAO role in Discord with Developer Mode on and copy ID
# ZAO_ROLE_ID=123456789012345678

# ZAO Channel ID (Required for ZAO features)
# Right-click the ZAO channel in Discord with Developer Mode on and copy ID
# ZAO_CHANNEL_ID=123456789012345678

# Web Integration
# WEB_WEBHOOK_URL=https://your-app.vercel.app/api/webhook
# WEBHOOK_SECRET=your_webhook_secret

# Debug Mode (Optional, default: false)
DEBUG=FALSE
